# MSG_MOVE_HEARTBEAT - Needs more testing
endian: little
header:
data:
    z: f
    x: f
    y: f
    move_flags: I
    fall_time_ms: I
    sequence_id: I
    test: 3B # unknown trio (possibly pitch/transport bits)
    facing: f # Facing is using rad. 

    pitch: f               # 0.0 in capture (likely pitch)
    jump_vertical_speed: f # -7.95555 in capture (jump Z-velocity)
    fall_time_ms_2: I      # 500 (0x01F4) in capture
    jump_cos: f            # 0.50785685 in capture (cos/sin of hop angle)
    client_time_2: I       # 0x11104BF7 in capture (second tick/ack)
