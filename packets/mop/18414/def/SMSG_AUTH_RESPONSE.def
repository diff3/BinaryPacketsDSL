endian: little
header:
  auth_ok: bits, 1BI
  realm_count: bits, 21BI

  loop €realm_count to realmlist:
    realm_name_length: bits, 8BI
    normalized_name_length: bits, 8BI
    is_home_realm: bits, 1BI
    playable_classes_count: bits, 23BI
    reserved_bit: bits, 21BI
    _ : bits, 4B
    playable_races_count: bits, 23BI
    _ : bits, 1B
    queued: bits, 1BI

  loop €realm_count to realmdata:
    realm_id: I
    realm_name: €realmlist[0].realm_name_length's
    normalized_name: €realmlist[0].normalized_name_length's

    loop €realmlist[0].playable_races_count to races:
      expansion: B
      race_id: B

    loop €realmlist[0].playable_classes_count to classes:
      expansion: B
      class_id: B

    _ : I
    server_expansion_id: I
    _reserved: I
    flags: I