{
    "name": "bpdlang-syntax",
    "displayName": "BPDLang Syntax Highlighting",
    "publisher": "local",
    "version": "0.0.29",
    "engines": {
        "vscode": "^1.50.0"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "contributes": {
        "languages": [
            {
                "id": "bpdlang",
                "aliases": [
                    "BPDLang",
                    "bpd"
                ],
                "extensions": [
                    ".bpd"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bpdlang",
                "scopeName": "source.bpdlang",
                "path": "./syntaxes/bpdlang.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "@vscode/vsce": "^3.3.2"
    }
}
